alias -s {md,js,ts,jsx,tsx,json,html,xml,css,scss,vue}=vim

# dir
alias ,c='cd ~/Code'
alias ,i='cd ~/ipalfish'

# sys

alias et='exit'
alias c='clear'
alias rm='rm -i'
alias l='ls -1AF'
alias rmf='rm -rf'
alias hs='history'
alias vi='nvim'
alias vim='nvim'


# git
alias ,grh!='git reset --hard HEAD'
alias gs='git status --short'
alias ga='git add'
alias gaa='git add -A'
alias gc='git commit'
alias gca='git commit -a'
alias gm='git merge'
alias gmt='git mergetool'
alias gb='echo "$(git branch)"'
alias gba='echo "$(git branch -a)"'
alias gd='git diff'
alias gdt='git difftool'
alias gf='git fetch origin'
alias glg='git log \
  --date=format:"%m/%d-%H:%M" \
  --pretty="%C(yellow)%h %<(12,trunc)%C(red)%ad %<(12,trunc)%C(blue)%cn %C(reset)%s"'

alias yarn='yarn --registry https://registry.npm.taobao.org/'
alias cyarn='yarn --registry https://registry.npm.taobao.org/'
