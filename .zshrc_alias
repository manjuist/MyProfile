export CDPATH=.:~:~/Desktop:~/Code

alias -s {js,ts,jsx,tsx,json,html,xml,css,scss,vue}=vim

alias ly="cd ~/Code/official_site_static"
alias h5="cd ~/Code/official_site_h5"
alias wx="cd ~/Code/wxapp-huazhu"
alias pc="cd ~/Code/pc-huazhu"
alias cl="cd ~/Code/lvyue-cli"
alias eb="cd ~/Code/ows_eb"

alias c="clear"
alias rm="rm -i"
alias mv="mv -i"
alias l="ls -1AF"
alias ll="ls -lhAF"
