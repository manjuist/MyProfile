# dir
export CDPATH=.:~:~/Desktop:~/Code

# sys
alias -s {js,ts,jsx,tsx,json,html,xml,css,scss,vue}=vim
alias ag='ag --path-to-ignore ~/.agignore'
alias ll="ls -lhAF"
alias l="ls -1AF"
alias rm="rm -i"
alias mv="mv -i"
alias c="clear"
alias vi="vim"

# git
alias glog='git log \
  --date=format:"%m/%d-%H:%M" \
  --pretty="%C(yellow)%h %<(12,trunc)%C(red)%ad %<(12,trunc)%C(blue)%cn %C(reset)%s"'
alias gba='echo "$(git branch -a)"'
alias gb='echo "$(git branch)"'
alias gca="git commit -a"
alias gco="git checkout"
alias gs="git status"
alias gc="git commit"
alias gm="git merge"
alias gd="git diff"
alias ga="git add"
