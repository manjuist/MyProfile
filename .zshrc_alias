# dir
CDPATH=~/Desktop/Code
# sys
alias vi='vim'
alias rm='rm -i'
alias mv='mv -i'
alias ag='ag --path-to-ignore ~/.agignore'
alias c='clear'
alias l1='ls -1AF'
alias ,l='ls -lAF'
alias ,c='cd ~/Code'
# git
alias ,gb='echo "$(git branch)"'
alias ,gba='echo "$(git branch -a)"'
alias ,glg='git log \
  --date=format:"%m/%d-%H:%M" \
  --pretty="%C(yellow)%h %<(12,trunc)%C(red)%ad %<(12,trunc)%C(blue)%cn %C(reset)%s"'
