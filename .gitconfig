[core]
	editor = nvim

[commit]
  template=~/.gitmessages
[merge]
	tool = diffmerge
[diff]
	tool = diffmerge
[mergetool "diffmerge"]
	path = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge
	trustExitCode = true
[difftool "diffmerge"]
	path = /Applications/DiffMerge.app/Contents/MacOS/DiffMerge
[difftool]
	prompt = false
